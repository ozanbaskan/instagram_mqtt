{
   "name": "obigmqtt",
   "version": "0.0.3",
   "description": "Realtime and Push Notification (FBNS) support for the instagram-private-api",
   "bugs": {
      "url": "https://github.com/ozanbaskan7/instagram_mqtt/issues"
   },
   "main": "dist/index.js",
   "repository": {
      "type": "git",
      "url": "https://github.com/ozanbaskan7/instagram_mqtt.git"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "build": "rimraf dist && tsc -p tsconfig.build.json",
      "prepare": "npm run build",
      "lint": "eslint --fix --ext .js,.ts src/**",
      "format": "prettier -w . --log-level warn",
      "check-format": "prettier -c .",
      "typecheck": "tsc -p tsconfig.build.json --noEmit",
      "precommit": "npm run typecheck && npm run lint && npm run format"
   },
   "engines": {
      "node": ">=18.0.0"
   },
   "keywords": [],
   "author": {
      "name": "Ozan Baskan",
      "email": "ozanbaskan7@gmail.com"
   },
   "license": "MIT",
   "dependencies": {
      "chance": "^1.1.11",
      "debug": "^4.3.4",
      "eventemitter3": "^5.0.1",
      "instagram-private-api": "^1.46.1",
      "obmqtt": "^0.0.2",
      "socks": "^2.8.1",
      "ts-custom-error": "^3.3.1"
   },
   "devDependencies": {
      "@types/chance": "^1.1.6",
      "@types/debug": "^4.1.12",
      "@types/luxon": "^3.4.2",
      "@types/node": "^24.10.1",
      "@typescript-eslint/eslint-plugin": "^7.5.0",
      "@typescript-eslint/parser": "^7.5.0",
      "eslint": "^8.57.0",
      "prettier": "^3.2.5",
      "rimraf": "^5.0.5",
      "ts-xor": "^1.3.0",
      "typescript": "^5.4.4"
   },
   "types": "dist/index.d.ts",
   "overrides": {
      "ts-custom-error": "^3.3.1"
   }
}
